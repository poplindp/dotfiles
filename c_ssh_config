Host *
    # Avoid freezing
    ServerAliveInterval 30
    ServerAliveCountMax 5
    ForwardAgent yes
    ControlMaster auto
    ControlPersist yes
    ControlPath ~/.ssh/socket-%r@%h:%p
    Compression yes
    AddressFamily inet
    ServerAliveInterval 120

